<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="default" extends="struts-default">
		<action name="login" class="com.projeto.modelo.action.UserAction">
			<result name="SUCCESS">pagamento.jsp</result>
			<result name="FAIL">loginfail.jsp</result>
			<result name="CADASTRO">register.jsp</result>
		</action>
		<action name="register" class="com.projeto.modelo.action.UserAction" method="registerSave">
			<result name="SUCCESS">registerendpage.jsp</result>
			<result name="FAIL">registerfail.jsp</result>		
		</action>	
		
		<action name="insertUser" class="com.projeto.modelo.action.UserAction" method="save">
			<result name="SUCCESS">finalregistro.jsp</result>
			<result name="FAIL">registerfail.jsp</result>		
		</action>		
		<action name="insertContato" class="com.projeto.modelo.action.ContatoAction" method="save">
			<result name="SUCCESS">finalcontato.jsp</result>
			<result name="FAIL">registerfail.jsp</result>		
		</action>	
		<action name="insertCarros" class="com.projeto.modelo.action.CarrosAction" method="save">
			<result name="SUCCESS">finalcontato.jsp</result>
		</action>	
		
		<action name="carrosDesconto" class="com.projeto.modelo.action.CarrosAction" method="pesqCarrosDesconto">
			<result name="SUCCESS">paginaprincipal.jsp</result>			
		</action>	
		<action name="detalhes" class="com.projeto.modelo.action.CarrosAction" method="detalhesCarro">
			<result name="SUCCESS">carrosDetalhes.jsp</result>			
		</action>
		<action name="todosCarros" class="com.projeto.modelo.action.CarrosAction" method="todosCarros">
			<result name="SUCCESS">carros.jsp</result>			
		</action>
		<action name="gerarLotoFacil" class="com.projeto.modelo.action.LotoFacilAction" method="gerarLotoFacil">
			<result name="SUCCESS">lotofacil.jsp</result>			
		</action>
		<action name="gravarLotoFacil" class="com.projeto.modelo.action.LotoFacilAction" method="gravarLotoFacil">
			<result name="SUCCESS">cadastrarlotofacil.jsp</result>			
		</action>
		<action name="gerarMegaSena" class="com.projeto.modelo.action.MegaSenaAction" method="gerarMegaSena">
			<result name="SUCCESS">megasena.jsp</result>			
		</action>
	</package>
</struts>